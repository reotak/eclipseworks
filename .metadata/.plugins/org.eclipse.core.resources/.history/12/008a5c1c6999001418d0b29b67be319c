package ex17_01;

import java.util.ArrayList;
import java.util.List;

class CheckMemory {

	public static void main(String[] args) {
		List<GabageObject> objs = new ArrayList<GabageObject>();

		for (int i = 0; i < 100000; i ++) {
			objs.add(new GabageObject(i));
		}
	}

	static class GabageObject {
		private int data;

		public GabageObject(int data) {
			this.data = data;
			System.out.println(" GO created : " + this.data);
		}

		protected void finalize() {
			System.out.println(" GO finalized : " + this.data);
		}
	}
}
