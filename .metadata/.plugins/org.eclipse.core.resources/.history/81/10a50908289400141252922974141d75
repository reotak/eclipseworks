package gui;

import java.lang.reflect.Type;

class ArgumentsSetWindowManagerForConsructor extends ArgumentsSetWindowManager{

	final private InstanceViewer instanceViewer;

	public ArgumentsSetWindowManagerForConsructor(Type[] argTypes, InstanceViewer instanceViewer) {
		super(argTypes);
		this.instanceViewer = instanceViewer;
	}

	@Override
	public void catchArguments(Object[] args) {
		System.out.println(args);
	}


}
