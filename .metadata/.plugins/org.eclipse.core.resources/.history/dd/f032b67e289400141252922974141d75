package gui;

import java.lang.reflect.Type;

class SetArgumentsWindowManagerForConsructor extends ArgumentsSetWindowManager{

	final private InstanceViewer instanceViewer;

	public SetArgumentsWindowManagerForConsructor(Type[] argTypes, InstanceViewer instanceViewer) {
		super(argTypes);
		this.instanceViewer = instanceViewer;
	}

	public void catchArguments(Object[] args) {
		System.out.println(args);
	}
}
